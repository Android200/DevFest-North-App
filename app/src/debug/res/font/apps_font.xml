<?xml version="1.0" encoding="utf-8"?>
<font-family xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">


    <!-- regular -->
    <font
        android:font="@font/googlesansregular"
        android:fontStyle="normal"
        android:fontWeight="400"

        app:font="@font/googlesansregular"
        app:fontStyle="normal"
        app:fontWeight="400" />

    <!-- italic -->
    <font
        android:font="@font/googlesansmedium"
        android:fontStyle="normal"
        android:fontWeight="500"

        app:font="@font/googlesansmedium"
        app:fontStyle="italic"
        app:fontWeight="500" />

    <!-- bold -->
    <font
        android:font="@font/googlesansbold"
        android:fontStyle="normal"
        android:fontWeight="700"

        app:font="@font/googlesansbold"
        app:fontStyle="normal"
        app:fontWeight="700" />

</font-family>